{
  "name": "irisactivation",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "migrate:assignments": "node scripts/migrate-assignments.js",
    "migrate:assignments:apply": "node scripts/migrate-assignments.js --apply",
    "deploy:rules": "npx firebase deploy --only firestore:rules",
    "test:rules": "jest --config jest-rules.config.cjs --runInBand",
    "test:rules:emu": "npx firebase emulators:exec --only firestore \"node ./node_modules/jest/bin/jest.js --config jest-rules.config.cjs --runInBand\"",
    "deploy:indexes": "npx firebase deploy --only firestore:indexes",
    "seed:quiz": "node scripts/seed-quiz-from-file.js seedQuizQuestionsNew.json",
    "backfill:audit-actors": "node scripts/backfill-audit-actors.js"
  },
  "jest": {
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "<rootDir>/rules-tests/"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/drawer": "^7.5.3",
    "@react-navigation/native": "^7.1.6",
    "expo": "^53.0.20",
    "expo-camera": "~16.1.10",
    "expo-clipboard": "^7.1.5",
    "expo-font": "~13.3.2",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-router": "^5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.2.0",
    "firebase": "^12.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gifted-charts": "^1.4.63",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.20.0",
    "sentry-expo": "~7.0.0",
    "expo-dev-client": "~5.2.4",
    "react-native-gesture-handler": "~2.24.0"
  },
  "devDependencies": {
    "firebase-admin": "^13.4.0",
    "@babel/core": "^7.25.2",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "eas-cli": "^16.17.4",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.9",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
