# Common caches and local-only folders
.expo/
.expo-shared/
dist/
web-build/
.metro-health-check*

# VCS
.git/
.gitignore
.gitattributes

# OS/editor
.DS_Store
.vscode/
.idea/

# Logs
npm-debug.*
yarn-debug.*
yarn-error.*

# Node modules (EAS reconstructs from package.json)
node_modules/

# Env files (set these in EAS secrets instead)
.env*

